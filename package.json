{
  "name": "headers-support",
  "version": "0.1.0",
  "license": "Apache-2.0 OR CC-BY-4.0",
  "description": "A set of elements that provides fill support for headers editor",
  "devDependencies": {
    "polymer-cli": "^0.16.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "private": false,
  "main": [
    "headers-support.html"
  ],
  "author": "The Advanced REST client authors <arc@mulesoft.com>",
  "scripts": {
    "serve": "node_modules/.bin/polymer serve",
    "lint": "node_modules/.bin/polymer lint headers-support-authorization.html headers-support-content-type.html headers-support-cookie.html headers-support-wsse.html",
    "test-sauce": "node_modules/.bin/polymer test --plugin sauce --job-name \"headers-support:local-test\"",
    "test": "node_modules/.bin/polymer test",
    "deps": "npm install --quiet && bower install --quiet",
    "update-deps": "npm update --quiet && bower update --quiet"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/advanced-rest-client/headers-support.git"
  },
  "bugs": {
    "url": "https://github.com/advanced-rest-client/headers-support/issues",
    "email": "arc@mulesoft.com"
  }
}
